<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">


	<!-- User Service -->
	<bean id="userService" class="com.qog.UserService">
		<constructor-arg name="jdao" ref="jdao" />
	</bean>

	<bean id="navService" class="com.qog.NavService">
		<constructor-arg name="jdao" ref="jdao" />
	</bean>

	<bean id="surveyService" class="com.qog.SurveyService">
		<constructor-arg name="jdao" ref="jdao" />
		<constructor-arg name="jtx" ref="jtx" />
	</bean>

	<bean id="questionService" class="com.qog.QuestionService">
		<constructor-arg name="jdao" ref="jdao" />
		<constructor-arg name="jtx" ref="jtx" />
	</bean>

	<bean id="answerService" class="com.qog.AnswerService">
		<constructor-arg name="jdao" ref="jdao" />
		<constructor-arg name="jtx" ref="jtx" />
	</bean>
</beans>
